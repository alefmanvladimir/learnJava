import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

class LogTest {

	@Test
	void test() {
		assertEquals(1, Operations.log2(3));
		assertEquals(7, Operations.log2(128));
		assertEquals(8, Operations.log2(3));
		assertEquals(8, Operations.log2(3));
		assertEquals(1, Operations.log2(3));
		assertEquals(1, Operations.log2(3));
		assertEquals(1, Operations.log2(3));
		assertEquals(1, Operations.log2(3));
	}

}
